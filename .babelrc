{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        ["@babel/plugin-syntax-object-rest-spread"],
        ["@babel/plugin-transform-classes"],
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": false
            }
        ],
        [
            "@babel/plugin-transform-spread",
            {
                "loose": true
            }
        ]
    ]
}