{
  "name": "@easylogic/sapa",
  "version": "0.5.3",
  "description": "Simple JS Application Library - Sapa",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./@types/index.d.ts",
  "module": "./dist/sapa.es.js",
  "directories": {
    ".": "./src"
  },
  "files": [
    "@types",
    "samples",
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/sapa.umd.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "release-it": "^15.6.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9",
    "vitest": "^0.5.5"
  },
  "scripts": {
    "build": "vite build --config vite.dist.config.js",
    "test": "vitest",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "release": "release-it *",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easylogic/sapa.git"
  },
  "keywords": [
    "sapa",
    "easylogic",
    "simple",
    "application",
    "library"
  ],
  "author": "easylogic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easylogic/sapa/issues"
  },
  "homepage": "https://github.com/easylogic/sapa"
}
